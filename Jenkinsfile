pipeline {
  agent any
  stages {
    stage('static code checks = linting') {
      steps {
        script {
          echo hh
        }

      }
    }
    stage('...') {
      parallel {
        stage('...') {
          steps {
            jiraIssueSelector()
          }
        }
        stage('other browser') {
          steps {
            sh 'ls'
          }
        }
        stage('') {
          steps {
            library 'kkljkl'
          }
        }
      }
    }
    stage('rpm gen') {
      steps {
        archiveArtifacts(artifacts: 'aarar', onlyIfSuccessful: true)
      }
    }
    stage('interactive)') {
      agent any
      environment {
        vvv = 'j'
      }
      steps {
        waitUntil()
        input 'bro?'
      }
    }
    stage('') {
      steps {
        retry(count: 1)
      }
    }
    stage('jjkjkl') {
      steps {
        sleep 4
      }
    }
  }
  environment {
    en = 'nnn'
  }
}